#
# Arabic (US-101 keys) keyboard layout.
#

type OVERLAY

### ROW 1

key GRAVE {
    label:                              '`'
    base:                               '\u0630'
    shift:                              '\u0651'
}

key 1 {
    label:                              '1'
    base:                               '\u0661'
    capslock:                           '1'
    shift:                              '!'
}

key 2 {
    label:                              '2'
    base:                               '\u0662'
    capslock:                           '2'
    shift:                              '@'
}

key 3 {
    label:                              '3'
    base:                               '\u0663'
    capslock:                           '3'
    shift:                              '#'
}

key 4 {
    label:                              '4'
    base:                               '\u0664'
    capslock:                           '4'
    shift:                              '$'
}

key 5 {
    label:                              '5'
    base:                               '\u0665'
    capslock:                           '5'
    shift:                              '\u066a'
    shift+capslock:                     '%'
}

key 6 {
    label:                              '6'
    base:                               '\u0666'
    capslock:                           '6'
    shift:                              '^'
}

key 7 {
    label:                              '7'
    base:                               '\u0667'
    capslock:                           '7'
    shift:                              '&'
}

key 8 {
    label:                              '8'
    base:                               '\u0668'
    capslock:                           '8'
    shift:                              '*'
}

key 9 {
    label:                              '9'
    base:                               '\u0669'
    capslock:                           '9'
    shift:                              ')'
}

key 0 {
    label:                              '0'
    base:                               '\u0660'
    capslock:                           '0'
    shift:                              '('
}

key MINUS {
    label:                              '-'
    base:                               '-'
    shift:                              '_'
}

key EQUALS {
    label:                              '='
    base:                               '='
    shift:                              '+'
}

