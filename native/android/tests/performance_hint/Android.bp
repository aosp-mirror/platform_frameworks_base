}

cc_test {
    name: "PerformanceHintNativeTestCases",

    multilib: {
        lib32: {
            suffix: "32",
        },
        lib64: {
            suffix: "64",
        },
    },

    srcs: ["PerformanceHintNativeTest.cpp"],

    shared_libs: [
        "libandroid",
        "liblog",
        "libbinder",
        "libpowermanager",
        "libutils",
    ],

    static_libs: [
        "libbase",
        "libgmock",
        "libgtest",
    ],
    stl: "c++_shared",

    test_suites: [
        "device-tests",
    ],

    cflags: [
        "-Werror",
        "-Wall",
    ],

    header_libs: [
        "libandroid_headers_private",
    ],
}
