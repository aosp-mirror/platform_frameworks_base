cc_test {
    name: "libandroidfw_tests",
    host_supported: true,

    cflags: [
        "-Wall",
        "-Werror",
        "-Wunused",
        "-Wunreachable-code",
        "-Wno-missing-field-initializers",
        // gtest is broken
        "-Wno-unnamed-type-template-args",
    ],

    srcs:  [
        "AttributeFinder_test.cpp",
        "ByteBucketArray_test.cpp",
        "Config_test.cpp",
        "ConfigLocale_test.cpp",
        "Idmap_test.cpp",
        "ResTable_test.cpp",
        "Split_test.cpp",
        "TestHelpers.cpp",
        "Theme_test.cpp",
        "TypeWrappers_test.cpp",
        "ZipUtils_test.cpp",
    ],
    target: {
        android: {
            srcs: [
                "BackupData_test.cpp",
                "ObbFile_test.cpp",
            ],
            shared_libs: [
                "libandroidfw",
                "libcutils",
                "libutils",
                "libui",
            ],
        },
        host: {
            static_libs: [
                "libandroidfw",
                "libutils",
                "libcutils",
                "liblog",
                "libz",
            ],
        },
    },

}
