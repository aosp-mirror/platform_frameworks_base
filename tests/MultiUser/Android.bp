android_test {
    name: "MultiUserTests",
    platform_apis: true,
    srcs: ["src/**/*.java"],

    static_libs: [
        "androidx.test.rules",
        "androidx.test.runner",
        "services.core",
    ],
    libs: [
        "android.test.runner",
        "android.test.base",
        "android.test.mock",
    ],
    certificate: "platform",
    test_suites: ["device-tests"],
}
