android_test {
    name: "FrameworkPermissionTests",
    // Include all test java files.
    srcs: ["src/**/*.java"],
    libs: [
        "android.test.runner",
        "android.test.base",
        "telephony-common",
    ],
    static_libs: [
        "androidx.test.runner",
        "junit",
        "platform-test-annotations",
    ],
    platform_apis: true,
    test_suites: ["device-tests"],
}
