    <shortcode country="ge" pattern="\\d{1,5}" premium="801[234]|888[239]" free="95201|95202|95203" />

    <!-- Ghana: 4 digits, known premium codes listed -->
    <shortcode country="gh" pattern="\\d{4}" free="5041|3777|2333|6061" />

